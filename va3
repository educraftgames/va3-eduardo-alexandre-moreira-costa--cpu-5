package main;

import java.util.ArrayList;
import java.util.List;
import java.util.Random;

public class masmorracomlista{

	public static void main(String[] args) {
		Random rand = new Random();
		
		Masmorra masmorra = new Masmorra();
		
		List<Masmorra> inicio = new ArrayList<Masmorra>();
		
		for(int i = 0; i < 5; i++) {
			Object MasmorraStatus;
			masmorra.add(rand.nextBoolean() 
					? new masmorra1(i*10, i, rand.nextBoolean() 
							? MasmorraStatus.inicio 
							: MasmorraStatus.lutando)
					: new Masmorra2(i*10, i, rand.nextBoolean()
							?MasmorraStatus.inicio 
							: MasmorraStatus,lutando));
		}
		
		for(int i = 0; i < inicio.size(); i++) {
			if(rand.nextBoolean())
				estacionamento.receberNave(masmorracomlista.get(i));
			else
				estacionamento.liberarNave(masmorracomlista.get(i));
		}

	}

}

